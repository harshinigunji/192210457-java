import java.util.Scanner;
public class Pp {
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input=new Scanner(System.in);
        int r=input.nextInt();
        int c=input.nextInt();
        for(int i=1;i<=r;i++)
        {
            for(int j=1;j<=c;j++)
            {
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
Input:
r=3
c=5
Output:
* * * * * 
* * * * * 
* * * * * 
