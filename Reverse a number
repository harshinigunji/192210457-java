import java.util.Scanner;
public class Pp {
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input=new Scanner(System.in);
        int n=input.nextInt();
        int rev=0;
        while(n>0)
        {
            int i=n%10;
            rev=rev*10+i;
            n=n/10;
        }
        System.out.println("Revese:"+rev);
    }
}
Input:
123
Output:
321
