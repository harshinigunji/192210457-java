import java.util.Scanner;
public class Pp {
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input=new Scanner(System.in);
        int n=input.nextInt();
        while(n%3==0)
        {
            n=n/3;
        }
        if(n==1)
        {
            System.out.println("Power of 3");
        }
        else
        {
            System.out.println("Not power of 3");
        }
    }
}
Input:
27
Output:
Power of 3
